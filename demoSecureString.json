{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "adminUserName": {
      "type": "string",
      "defaultValue": null,
      "metadata": {
        "description": "Administrative username used for creating VMs."
      }
    },
    "pwSecure": {
      "type": "securestring",
      "metadata": {
        "description": "PowerShell PSCredential object password that is encrypted."
      }
    }
  },
  "variables": {},
  "resources": [
  ],
  "outputs": {
    "adminUserName": {
      "type": "string",
      "value": "[parameters('adminUserName')]"
    },
    "pwSecure": {
      "type": "securestring",
      "value": "[parameters('pwSecure')]"
    }
  }
}
